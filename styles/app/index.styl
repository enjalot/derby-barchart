@import "../base";


body {
  font: 14px helvetica;
  color: #f0f0f0;
  background-color: #333;
}

.row {
  padding: 5px;
  margin: 0px;
}

.axis path,
.axis line {
  fill: none;
  stroke: #f0f0f0;
  shape-rendering: crispEdges;
}

.axis text {
  fill: #f0f0f0;
}

.brush .extent {
  stroke: #f0f0f0;
  fill-opacity: .125;
  shape-rendering: crispEdges;
}

.bar {
  fill: #5EB4E3;
}
.selected {
  fill: #78C656;
}

.arrows {
  position:relative;

}

.arrows .arrow {
  float:left;
  margin-top: 20px;
  margin-left: 20px;
  color: white;
}
.arrow a, .arrow a:visited, .arrow a:hover {
  color: white
}
.arrows .up {
  margin: 20px;
}
.arrows .down {
  margin:10px;
}
